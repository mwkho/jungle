<section class="cart-show">

  <header class="page-header">
    <h1>My Cart</h1>
  </header>
  <% if enhanced_cart.empty?%>
    <p class='alert alert-warning'> You have an empty cart, click <%= link_to 'here', products_path%> to shop for an item </p> 
  <% else%>
    <%= render 'cart_list' %>
  <% end%>
</section>
